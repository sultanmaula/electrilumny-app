<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.electrilumnyy">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Electrilumnyy"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".UserSide.User_Setting"
            android:exported="false" />
        <activity
            android:name=".UserSide.User_Event"
            android:exported="false" />
        <activity
            android:name=".UserSide.User_DataAlumni"
            android:exported="false" />
        <activity
            android:name=".AdminSide.Setting_Admin"
            android:exported="false" />
        <activity
            android:name=".AdminSide.Event_Admin"
            android:exported="false" />
        <activity
            android:name=".AdminSide.DataAlumni_Admin"
            android:exported="false" />
        <activity
            android:name=".UserSide.Dashboard_User"
            android:exported="false"
            android:label="@string/title_activity_dashboard_user" />
        <activity
            android:name=".AdminSide.Dashboard_Admin"
            android:exported="false"
            android:label="@string/title_activity_dashboard_admin" />
        <activity
            android:name=".User_Dashboard"
            android:exported="false"
            android:label="@string/title_activity_user_dashboard" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>